selected_fg_color:#C7DBDD
tooltip_bg_color:#0e3c46
tooltip_fg_color:#C7DBDD
titlebar_bg_color:#0e3c46
titlebar_fg_color:#C7DBDD
menu_bg_color:#0e3c46
menu_fg_color:#C7DBDD
