<?xml version="1.0" encoding="UTF-8"?>
<style-scheme version="1.0" name="Base16 Horizon Dark">
  <!--
  base16-qtcreator (https://github.com/ilpianista/base16-qtcreator)
  by Chris Kempson (http://chriskempson.com)
  Horizon Dark scheme by MichaÃ«l Ball (http://github.com/michael-ball/)
  -->

  <!-- main colors, skipping one of attributes (backgroud/foreground) will use color from here -->
  <style name="Text" foreground="#CBCED0" background="#1C1E26"/>

  <!-- it will be underlined by default -->
  <style name="Link"/>

  <!-- selection, current line, search, matching brackets -->
  <style name="Selection" background="#2E303E"/>
  <style name="CurrentLine" background="#6F6F70"/>
  <style name="LineNumber" foreground="#6F6F70"/>
  <style name="CurrentLineNumber" foreground="#9DA0A2" bold="true"/>
  <style name="SearchResult" background="#232530"/>
  <style name="Parentheses" background="#2E303E" />
  <style name="ParenthesesMismatch" background="#B072D1" />

  <!-- Selection whilst searching in it -->
  <style name="SearchScope" foreground="#E3E6EE" background="#6F6F70"/>

  <!-- word (symbol) under cursor: background - color of marking frame, foreground - ignored -->
  <style name="Occurrences" background="#6F6F70"/>
  <style name="Occurrences.Rename" background="#9DA0A2"/>
  <!-- unused variable: foreground - color of underlining -->
  <style name="Occurrences.Unused" foreground="#E58D7D"/>

  <!-- language primitives -->
  <style name="Number" foreground="#E58D7D"/>
  <style name="String" foreground="#EFAF8E"/>
  <style name="Keyword" foreground="#B072D1"/>
  <style name="Operator" foreground="#CBCED0"/>
  <style name="Preprocessor" foreground="#DF5273"/>
  <style name="Label" foreground="#E93C58"/>

  <!-- types, classes -->
  <style name="Type" foreground="#EFB993"/>
  <style name="PrimitiveType" foreground="#B072D1"/>

  <!-- functions, methods -->
  <style name="Function" foreground="#DF5273"/>
  <style name="VirtualMethod" foreground="#DF5273" italic="true"/>

  <!-- variables -->
  <style name="Field" foreground="#DCDFE4"/>
  <style name="Local" foreground="#CBCED0"/>
  <style name="Global" foreground="#CBCED0"/>
  <style name="Static" foreground="#CBCED0"/>
  <style name="JsScopeVar" foreground="#CBCED0" italic="true"/>
  <style name="JsImportVar" foreground="#CBCED0" italic="true"/>
  <style name="JsGlobalVar" foreground="#CBCED0" italic="true"/>

  <!-- whitespace -->
  <style name="VisualWhitespace" foreground="#2E303E"/>

  <!-- comments -->
  <style name="Comment" foreground="#6F6F70"/>
  <style name="DisabledCode" foreground="#6F6F70"/>
  <style name="Doxygen.Comment" foreground="#6F6F70"/>
  <style name="Doxygen.Tag" foreground="#6F6F70"/>

  <!-- QML -->
  <style name="QmlTypeId" foreground="#B072D1"/>
  <style name="Binding" foreground="#E93C58"/>
  <style name="QmlExternalId" foreground="#DF5273" italic="true"/>
  <style name="QmlExternalObjectProperty" foreground="#DF5273" italic="true"/>
  <style name="QmlLocalId" italic="true"/>
  <style name="QmlRootObjectProperty" italic="true"/>
  <style name="QmlScopeObjectProperty" italic="true"/>
  <style name="QmlStateName" italic="true"/>

  <!-- inline diff -->
  <style name="DiffFile" foreground="#B072D1"/>
  <style name="DiffLocation" foreground="#DF5273"/>
  <style name="AddedLine" foreground="#EFAF8E"/>
  <style name="RemovedLine" foreground="#EFB993"/>
  <style name="LogChangeLine" foreground="#E93C58"/>

  <!-- side-by-side diff -->
  <style name="DiffFileLine" foreground="#B072D1"/>
  <style name="DiffContextLine" foreground="#DF5273"/>
  <style name="DiffSourceLine" foreground="#EFAF8E"/>
  <style name="DiffSourceChar" foreground="#EFB993" background="#232530"/>
  <style name="DiffDestLine" foreground="#EFAF8E"/>
  <style name="DiffDestChar" foreground="#EFB993" background="#232530"/>

  <!-- errors, warnings -->
  <style name="Error" underlineColor="#E93C58" underlineStyle="SingleUnderline"/>
  <style name="ErrorContext" underlineColor="#E93C58" underlineStyle="DotLine"/>
  <style name="Warning" underlineColor="#EFB993" underlineStyle="SingleUnderline"/>
  <style name="WarningContext" underlineColor="#EFB993" underlineStyle="DotLine"/>
</style-scheme>
