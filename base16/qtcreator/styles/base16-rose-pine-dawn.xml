<?xml version="1.0" encoding="UTF-8"?>
<style-scheme version="1.0" name="Base16 Rosé Pine Dawn">
  <!--
  base16-qtcreator (https://github.com/ilpianista/base16-qtcreator)
  by Chris Kempson (http://chriskempson.com)
  Rosé Pine Dawn scheme by Emilia Dunfelt &lt;edun@dunfelt.se&gt;
  -->

  <!-- main colors, skipping one of attributes (backgroud/foreground) will use color from here -->
  <style name="Text" foreground="#575279" background="#faf4ed"/>

  <!-- it will be underlined by default -->
  <style name="Link"/>

  <!-- selection, current line, search, matching brackets -->
  <style name="Selection" background="#f2e9de"/>
  <style name="CurrentLine" background="#9893a5"/>
  <style name="LineNumber" foreground="#9893a5"/>
  <style name="CurrentLineNumber" foreground="#797593" bold="true"/>
  <style name="SearchResult" background="#fffaf3"/>
  <style name="Parentheses" background="#f2e9de" />
  <style name="ParenthesesMismatch" background="#ea9d34" />

  <!-- Selection whilst searching in it -->
  <style name="SearchScope" foreground="#cecacd" background="#9893a5"/>

  <!-- word (symbol) under cursor: background - color of marking frame, foreground - ignored -->
  <style name="Occurrences" background="#9893a5"/>
  <style name="Occurrences.Rename" background="#797593"/>
  <!-- unused variable: foreground - color of underlining -->
  <style name="Occurrences.Unused" foreground="#ea9d34"/>

  <!-- language primitives -->
  <style name="Number" foreground="#ea9d34"/>
  <style name="String" foreground="#286983"/>
  <style name="Keyword" foreground="#ea9d34"/>
  <style name="Operator" foreground="#575279"/>
  <style name="Preprocessor" foreground="#907aa9"/>
  <style name="Label" foreground="#b4637a"/>

  <!-- types, classes -->
  <style name="Type" foreground="#d7827e"/>
  <style name="PrimitiveType" foreground="#ea9d34"/>

  <!-- functions, methods -->
  <style name="Function" foreground="#907aa9"/>
  <style name="VirtualMethod" foreground="#907aa9" italic="true"/>

  <!-- variables -->
  <style name="Field" foreground="#575279"/>
  <style name="Local" foreground="#575279"/>
  <style name="Global" foreground="#575279"/>
  <style name="Static" foreground="#575279"/>
  <style name="JsScopeVar" foreground="#575279" italic="true"/>
  <style name="JsImportVar" foreground="#575279" italic="true"/>
  <style name="JsGlobalVar" foreground="#575279" italic="true"/>

  <!-- whitespace -->
  <style name="VisualWhitespace" foreground="#f2e9de"/>

  <!-- comments -->
  <style name="Comment" foreground="#9893a5"/>
  <style name="DisabledCode" foreground="#9893a5"/>
  <style name="Doxygen.Comment" foreground="#9893a5"/>
  <style name="Doxygen.Tag" foreground="#9893a5"/>

  <!-- QML -->
  <style name="QmlTypeId" foreground="#ea9d34"/>
  <style name="Binding" foreground="#b4637a"/>
  <style name="QmlExternalId" foreground="#907aa9" italic="true"/>
  <style name="QmlExternalObjectProperty" foreground="#907aa9" italic="true"/>
  <style name="QmlLocalId" italic="true"/>
  <style name="QmlRootObjectProperty" italic="true"/>
  <style name="QmlScopeObjectProperty" italic="true"/>
  <style name="QmlStateName" italic="true"/>

  <!-- inline diff -->
  <style name="DiffFile" foreground="#ea9d34"/>
  <style name="DiffLocation" foreground="#907aa9"/>
  <style name="AddedLine" foreground="#286983"/>
  <style name="RemovedLine" foreground="#d7827e"/>
  <style name="LogChangeLine" foreground="#b4637a"/>

  <!-- side-by-side diff -->
  <style name="DiffFileLine" foreground="#ea9d34"/>
  <style name="DiffContextLine" foreground="#907aa9"/>
  <style name="DiffSourceLine" foreground="#286983"/>
  <style name="DiffSourceChar" foreground="#d7827e" background="#fffaf3"/>
  <style name="DiffDestLine" foreground="#286983"/>
  <style name="DiffDestChar" foreground="#d7827e" background="#fffaf3"/>

  <!-- errors, warnings -->
  <style name="Error" underlineColor="#b4637a" underlineStyle="SingleUnderline"/>
  <style name="ErrorContext" underlineColor="#b4637a" underlineStyle="DotLine"/>
  <style name="Warning" underlineColor="#d7827e" underlineStyle="SingleUnderline"/>
  <style name="WarningContext" underlineColor="#d7827e" underlineStyle="DotLine"/>
</style-scheme>
