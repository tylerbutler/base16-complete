<?xml version="1.0" encoding="UTF-8"?>
<style-scheme version="1.0" name="Base16 Rosé Pine Dawn">
  <!--
  base16-qtcreator (https://github.com/ilpianista/base16-qtcreator)
  by Chris Kempson (http://chriskempson.com)
  Rosé Pine Dawn scheme by Emilia Dunfelt &lt;sayhi@dunfelt.se&gt;
  -->

  <!-- main colors, skipping one of attributes (backgroud/foreground) will use color from here -->
  <style name="Text" foreground="#575279" background="#faf4ed"/>

  <!-- it will be underlined by default -->
  <style name="Link"/>

  <!-- selection, current line, search, matching brackets -->
  <style name="Selection" background="#f2e9de"/>
  <style name="CurrentLine" background="#9893a5"/>
  <style name="LineNumber" foreground="#9893a5"/>
  <style name="CurrentLineNumber" foreground="#6e6a86" bold="true"/>
  <style name="SearchResult" background="#fffaf3"/>
  <style name="Parentheses" background="#f2e9de" />
  <style name="ParenthesesMismatch" background="#907aa9" />

  <!-- Selection whilst searching in it -->
  <style name="SearchScope" foreground="#26233a" background="#9893a5"/>

  <!-- word (symbol) under cursor: background - color of marking frame, foreground - ignored -->
  <style name="Occurrences" background="#9893a5"/>
  <style name="Occurrences.Rename" background="#6e6a86"/>
  <!-- unused variable: foreground - color of underlining -->
  <style name="Occurrences.Unused" foreground="#b4637a"/>

  <!-- language primitives -->
  <style name="Number" foreground="#b4637a"/>
  <style name="String" foreground="#d7827e"/>
  <style name="Keyword" foreground="#907aa9"/>
  <style name="Operator" foreground="#575279"/>
  <style name="Preprocessor" foreground="#56949f"/>
  <style name="Label" foreground="#1f1d2e"/>

  <!-- types, classes -->
  <style name="Type" foreground="#ea9d34"/>
  <style name="PrimitiveType" foreground="#907aa9"/>

  <!-- functions, methods -->
  <style name="Function" foreground="#56949f"/>
  <style name="VirtualMethod" foreground="#56949f" italic="true"/>

  <!-- variables -->
  <style name="Field" foreground="#555169"/>
  <style name="Local" foreground="#575279"/>
  <style name="Global" foreground="#575279"/>
  <style name="Static" foreground="#575279"/>
  <style name="JsScopeVar" foreground="#575279" italic="true"/>
  <style name="JsImportVar" foreground="#575279" italic="true"/>
  <style name="JsGlobalVar" foreground="#575279" italic="true"/>

  <!-- whitespace -->
  <style name="VisualWhitespace" foreground="#f2e9de"/>

  <!-- comments -->
  <style name="Comment" foreground="#9893a5"/>
  <style name="DisabledCode" foreground="#9893a5"/>
  <style name="Doxygen.Comment" foreground="#9893a5"/>
  <style name="Doxygen.Tag" foreground="#9893a5"/>

  <!-- QML -->
  <style name="QmlTypeId" foreground="#907aa9"/>
  <style name="Binding" foreground="#1f1d2e"/>
  <style name="QmlExternalId" foreground="#56949f" italic="true"/>
  <style name="QmlExternalObjectProperty" foreground="#56949f" italic="true"/>
  <style name="QmlLocalId" italic="true"/>
  <style name="QmlRootObjectProperty" italic="true"/>
  <style name="QmlScopeObjectProperty" italic="true"/>
  <style name="QmlStateName" italic="true"/>

  <!-- inline diff -->
  <style name="DiffFile" foreground="#907aa9"/>
  <style name="DiffLocation" foreground="#56949f"/>
  <style name="AddedLine" foreground="#d7827e"/>
  <style name="RemovedLine" foreground="#ea9d34"/>
  <style name="LogChangeLine" foreground="#1f1d2e"/>

  <!-- side-by-side diff -->
  <style name="DiffFileLine" foreground="#907aa9"/>
  <style name="DiffContextLine" foreground="#56949f"/>
  <style name="DiffSourceLine" foreground="#d7827e"/>
  <style name="DiffSourceChar" foreground="#ea9d34" background="#fffaf3"/>
  <style name="DiffDestLine" foreground="#d7827e"/>
  <style name="DiffDestChar" foreground="#ea9d34" background="#fffaf3"/>

  <!-- errors, warnings -->
  <style name="Error" underlineColor="#1f1d2e" underlineStyle="SingleUnderline"/>
  <style name="ErrorContext" underlineColor="#1f1d2e" underlineStyle="DotLine"/>
  <style name="Warning" underlineColor="#ea9d34" underlineStyle="SingleUnderline"/>
  <style name="WarningContext" underlineColor="#ea9d34" underlineStyle="DotLine"/>
</style-scheme>
