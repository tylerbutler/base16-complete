<?xml version="1.0" encoding="UTF-8"?>
<style-scheme version="1.0" name="Base16 Windows 10 Light">
  <!--
  base16-qtcreator (https://github.com/ilpianista/base16-qtcreator)
  by Chris Kempson (http://chriskempson.com)
  Windows 10 Light scheme by Fergus Collins (https://github.com/C-Fergus)
  -->

  <!-- main colors, skipping one of attributes (backgroud/foreground) will use color from here -->
  <style name="Text" foreground="#767676" background="#f2f2f2"/>

  <!-- it will be underlined by default -->
  <style name="Link"/>

  <!-- selection, current line, search, matching brackets -->
  <style name="Selection" background="#d9d9d9"/>
  <style name="CurrentLine" background="#cccccc"/>
  <style name="LineNumber" foreground="#cccccc"/>
  <style name="CurrentLineNumber" foreground="#ababab" bold="true"/>
  <style name="SearchResult" background="#e5e5e5"/>
  <style name="Parentheses" background="#d9d9d9" />
  <style name="ParenthesesMismatch" background="#881798" />

  <!-- Selection whilst searching in it -->
  <style name="SearchScope" foreground="#0c0c0c" background="#cccccc"/>

  <!-- word (symbol) under cursor: background - color of marking frame, foreground - ignored -->
  <style name="Occurrences" background="#cccccc"/>
  <style name="Occurrences.Rename" background="#ababab"/>
  <!-- unused variable: foreground - color of underlining -->
  <style name="Occurrences.Unused" foreground="#f9f1a5"/>

  <!-- language primitives -->
  <style name="Number" foreground="#f9f1a5"/>
  <style name="String" foreground="#13a10e"/>
  <style name="Keyword" foreground="#881798"/>
  <style name="Operator" foreground="#767676"/>
  <style name="Preprocessor" foreground="#0037da"/>
  <style name="Label" foreground="#c50f1f"/>

  <!-- types, classes -->
  <style name="Type" foreground="#c19c00"/>
  <style name="PrimitiveType" foreground="#881798"/>

  <!-- functions, methods -->
  <style name="Function" foreground="#0037da"/>
  <style name="VirtualMethod" foreground="#0037da" italic="true"/>

  <!-- variables -->
  <style name="Field" foreground="#414141"/>
  <style name="Local" foreground="#767676"/>
  <style name="Global" foreground="#767676"/>
  <style name="Static" foreground="#767676"/>
  <style name="JsScopeVar" foreground="#767676" italic="true"/>
  <style name="JsImportVar" foreground="#767676" italic="true"/>
  <style name="JsGlobalVar" foreground="#767676" italic="true"/>

  <!-- whitespace -->
  <style name="VisualWhitespace" foreground="#d9d9d9"/>

  <!-- comments -->
  <style name="Comment" foreground="#cccccc"/>
  <style name="DisabledCode" foreground="#cccccc"/>
  <style name="Doxygen.Comment" foreground="#cccccc"/>
  <style name="Doxygen.Tag" foreground="#cccccc"/>

  <!-- QML -->
  <style name="QmlTypeId" foreground="#881798"/>
  <style name="Binding" foreground="#c50f1f"/>
  <style name="QmlExternalId" foreground="#0037da" italic="true"/>
  <style name="QmlExternalObjectProperty" foreground="#0037da" italic="true"/>
  <style name="QmlLocalId" italic="true"/>
  <style name="QmlRootObjectProperty" italic="true"/>
  <style name="QmlScopeObjectProperty" italic="true"/>
  <style name="QmlStateName" italic="true"/>

  <!-- inline diff -->
  <style name="DiffFile" foreground="#881798"/>
  <style name="DiffLocation" foreground="#0037da"/>
  <style name="AddedLine" foreground="#13a10e"/>
  <style name="RemovedLine" foreground="#c19c00"/>
  <style name="LogChangeLine" foreground="#c50f1f"/>

  <!-- side-by-side diff -->
  <style name="DiffFileLine" foreground="#881798"/>
  <style name="DiffContextLine" foreground="#0037da"/>
  <style name="DiffSourceLine" foreground="#13a10e"/>
  <style name="DiffSourceChar" foreground="#c19c00" background="#e5e5e5"/>
  <style name="DiffDestLine" foreground="#13a10e"/>
  <style name="DiffDestChar" foreground="#c19c00" background="#e5e5e5"/>

  <!-- errors, warnings -->
  <style name="Error" underlineColor="#c50f1f" underlineStyle="SingleUnderline"/>
  <style name="ErrorContext" underlineColor="#c50f1f" underlineStyle="DotLine"/>
  <style name="Warning" underlineColor="#c19c00" underlineStyle="SingleUnderline"/>
  <style name="WarningContext" underlineColor="#c19c00" underlineStyle="DotLine"/>
</style-scheme>
