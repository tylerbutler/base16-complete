<?xml version="1.0" encoding="UTF-8"?>
<style-scheme version="1.0" name="Base16 Windows 95">
  <!--
  base16-qtcreator (https://github.com/ilpianista/base16-qtcreator)
  by Chris Kempson (http://chriskempson.com)
  Windows 95 scheme by Fergus Collins (https://github.com/C-Fergus)
  -->

  <!-- main colors, skipping one of attributes (backgroud/foreground) will use color from here -->
  <style name="Text" foreground="#a8a8a8" background="#000000"/>

  <!-- it will be underlined by default -->
  <style name="Link"/>

  <!-- selection, current line, search, matching brackets -->
  <style name="Selection" background="#383838"/>
  <style name="CurrentLine" background="#545454"/>
  <style name="LineNumber" foreground="#545454"/>
  <style name="CurrentLineNumber" foreground="#7e7e7e" bold="true"/>
  <style name="SearchResult" background="#1C1C1C"/>
  <style name="Parentheses" background="#383838" />
  <style name="ParenthesesMismatch" background="#fc54fc" />

  <!-- Selection whilst searching in it -->
  <style name="SearchScope" foreground="#fcfcfc" background="#545454"/>

  <!-- word (symbol) under cursor: background - color of marking frame, foreground - ignored -->
  <style name="Occurrences" background="#545454"/>
  <style name="Occurrences.Rename" background="#7e7e7e"/>
  <!-- unused variable: foreground - color of underlining -->
  <style name="Occurrences.Unused" foreground="#a85400"/>

  <!-- language primitives -->
  <style name="Number" foreground="#a85400"/>
  <style name="String" foreground="#54fc54"/>
  <style name="Keyword" foreground="#fc54fc"/>
  <style name="Operator" foreground="#a8a8a8"/>
  <style name="Preprocessor" foreground="#5454fc"/>
  <style name="Label" foreground="#fc5454"/>

  <!-- types, classes -->
  <style name="Type" foreground="#fcfc54"/>
  <style name="PrimitiveType" foreground="#fc54fc"/>

  <!-- functions, methods -->
  <style name="Function" foreground="#5454fc"/>
  <style name="VirtualMethod" foreground="#5454fc" italic="true"/>

  <!-- variables -->
  <style name="Field" foreground="#d2d2d2"/>
  <style name="Local" foreground="#a8a8a8"/>
  <style name="Global" foreground="#a8a8a8"/>
  <style name="Static" foreground="#a8a8a8"/>
  <style name="JsScopeVar" foreground="#a8a8a8" italic="true"/>
  <style name="JsImportVar" foreground="#a8a8a8" italic="true"/>
  <style name="JsGlobalVar" foreground="#a8a8a8" italic="true"/>

  <!-- whitespace -->
  <style name="VisualWhitespace" foreground="#383838"/>

  <!-- comments -->
  <style name="Comment" foreground="#545454"/>
  <style name="DisabledCode" foreground="#545454"/>
  <style name="Doxygen.Comment" foreground="#545454"/>
  <style name="Doxygen.Tag" foreground="#545454"/>

  <!-- QML -->
  <style name="QmlTypeId" foreground="#fc54fc"/>
  <style name="Binding" foreground="#fc5454"/>
  <style name="QmlExternalId" foreground="#5454fc" italic="true"/>
  <style name="QmlExternalObjectProperty" foreground="#5454fc" italic="true"/>
  <style name="QmlLocalId" italic="true"/>
  <style name="QmlRootObjectProperty" italic="true"/>
  <style name="QmlScopeObjectProperty" italic="true"/>
  <style name="QmlStateName" italic="true"/>

  <!-- inline diff -->
  <style name="DiffFile" foreground="#fc54fc"/>
  <style name="DiffLocation" foreground="#5454fc"/>
  <style name="AddedLine" foreground="#54fc54"/>
  <style name="RemovedLine" foreground="#fcfc54"/>
  <style name="LogChangeLine" foreground="#fc5454"/>

  <!-- side-by-side diff -->
  <style name="DiffFileLine" foreground="#fc54fc"/>
  <style name="DiffContextLine" foreground="#5454fc"/>
  <style name="DiffSourceLine" foreground="#54fc54"/>
  <style name="DiffSourceChar" foreground="#fcfc54" background="#1C1C1C"/>
  <style name="DiffDestLine" foreground="#54fc54"/>
  <style name="DiffDestChar" foreground="#fcfc54" background="#1C1C1C"/>

  <!-- errors, warnings -->
  <style name="Error" underlineColor="#fc5454" underlineStyle="SingleUnderline"/>
  <style name="ErrorContext" underlineColor="#fc5454" underlineStyle="DotLine"/>
  <style name="Warning" underlineColor="#fcfc54" underlineStyle="SingleUnderline"/>
  <style name="WarningContext" underlineColor="#fcfc54" underlineStyle="DotLine"/>
</style-scheme>
