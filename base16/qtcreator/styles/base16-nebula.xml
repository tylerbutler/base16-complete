<?xml version="1.0" encoding="UTF-8"?>
<style-scheme version="1.0" name="Base16 Nebula">
  <!--
  base16-qtcreator (https://github.com/ilpianista/base16-qtcreator)
  by Chris Kempson (http://chriskempson.com)
  Nebula scheme by Gabriel Fontes (https://github.com/Misterio77)
  -->

  <!-- main colors, skipping one of attributes (backgroud/foreground) will use color from here -->
  <style name="Text" foreground="#a4a6a9" background="#22273b"/>

  <!-- it will be underlined by default -->
  <style name="Link"/>

  <!-- selection, current line, search, matching brackets -->
  <style name="Selection" background="#5a8380"/>
  <style name="CurrentLine" background="#6e6f72"/>
  <style name="LineNumber" foreground="#6e6f72"/>
  <style name="CurrentLineNumber" foreground="#87888b" bold="true"/>
  <style name="SearchResult" background="#414f60"/>
  <style name="Parentheses" background="#5a8380" />
  <style name="ParenthesesMismatch" background="#716cae" />

  <!-- Selection whilst searching in it -->
  <style name="SearchScope" foreground="#8dbdaa" background="#6e6f72"/>

  <!-- word (symbol) under cursor: background - color of marking frame, foreground - ignored -->
  <style name="Occurrences" background="#6e6f72"/>
  <style name="Occurrences.Rename" background="#87888b"/>
  <!-- unused variable: foreground - color of underlining -->
  <style name="Occurrences.Unused" foreground="#94929e"/>

  <!-- language primitives -->
  <style name="Number" foreground="#94929e"/>
  <style name="String" foreground="#6562a8"/>
  <style name="Keyword" foreground="#716cae"/>
  <style name="Operator" foreground="#a4a6a9"/>
  <style name="Preprocessor" foreground="#4d6bb6"/>
  <style name="Label" foreground="#777abc"/>

  <!-- types, classes -->
  <style name="Type" foreground="#4f9062"/>
  <style name="PrimitiveType" foreground="#716cae"/>

  <!-- functions, methods -->
  <style name="Function" foreground="#4d6bb6"/>
  <style name="VirtualMethod" foreground="#4d6bb6" italic="true"/>

  <!-- variables -->
  <style name="Field" foreground="#c7c9cd"/>
  <style name="Local" foreground="#a4a6a9"/>
  <style name="Global" foreground="#a4a6a9"/>
  <style name="Static" foreground="#a4a6a9"/>
  <style name="JsScopeVar" foreground="#a4a6a9" italic="true"/>
  <style name="JsImportVar" foreground="#a4a6a9" italic="true"/>
  <style name="JsGlobalVar" foreground="#a4a6a9" italic="true"/>

  <!-- whitespace -->
  <style name="VisualWhitespace" foreground="#5a8380"/>

  <!-- comments -->
  <style name="Comment" foreground="#6e6f72"/>
  <style name="DisabledCode" foreground="#6e6f72"/>
  <style name="Doxygen.Comment" foreground="#6e6f72"/>
  <style name="Doxygen.Tag" foreground="#6e6f72"/>

  <!-- QML -->
  <style name="QmlTypeId" foreground="#716cae"/>
  <style name="Binding" foreground="#777abc"/>
  <style name="QmlExternalId" foreground="#4d6bb6" italic="true"/>
  <style name="QmlExternalObjectProperty" foreground="#4d6bb6" italic="true"/>
  <style name="QmlLocalId" italic="true"/>
  <style name="QmlRootObjectProperty" italic="true"/>
  <style name="QmlScopeObjectProperty" italic="true"/>
  <style name="QmlStateName" italic="true"/>

  <!-- inline diff -->
  <style name="DiffFile" foreground="#716cae"/>
  <style name="DiffLocation" foreground="#4d6bb6"/>
  <style name="AddedLine" foreground="#6562a8"/>
  <style name="RemovedLine" foreground="#4f9062"/>
  <style name="LogChangeLine" foreground="#777abc"/>

  <!-- side-by-side diff -->
  <style name="DiffFileLine" foreground="#716cae"/>
  <style name="DiffContextLine" foreground="#4d6bb6"/>
  <style name="DiffSourceLine" foreground="#6562a8"/>
  <style name="DiffSourceChar" foreground="#4f9062" background="#414f60"/>
  <style name="DiffDestLine" foreground="#6562a8"/>
  <style name="DiffDestChar" foreground="#4f9062" background="#414f60"/>

  <!-- errors, warnings -->
  <style name="Error" underlineColor="#777abc" underlineStyle="SingleUnderline"/>
  <style name="ErrorContext" underlineColor="#777abc" underlineStyle="DotLine"/>
  <style name="Warning" underlineColor="#4f9062" underlineStyle="SingleUnderline"/>
  <style name="WarningContext" underlineColor="#4f9062" underlineStyle="DotLine"/>
</style-scheme>
