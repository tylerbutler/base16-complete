<?xml version="1.0" encoding="UTF-8"?>
<style-scheme version="1.0" name="Base16 dirtysea">
  <!--
  base16-qtcreator (https://github.com/ilpianista/base16-qtcreator)
  by Chris Kempson (http://chriskempson.com)
  dirtysea scheme by Kahlil (Kal) Hodgson
  -->

  <!-- main colors, skipping one of attributes (backgroud/foreground) will use color from here -->
  <style name="Text" foreground="#000000" background="#e0e0e0"/>

  <!-- it will be underlined by default -->
  <style name="Link"/>

  <!-- selection, current line, search, matching brackets -->
  <style name="Selection" background="#d0d0d0"/>
  <style name="CurrentLine" background="#707070"/>
  <style name="LineNumber" foreground="#707070"/>
  <style name="CurrentLineNumber" foreground="#202020" bold="true"/>
  <style name="SearchResult" background="#d0dad0"/>
  <style name="Parentheses" background="#d0d0d0" />
  <style name="ParenthesesMismatch" background="#000090" />

  <!-- Selection whilst searching in it -->
  <style name="SearchScope" foreground="#c4d9c4" background="#707070"/>

  <!-- word (symbol) under cursor: background - color of marking frame, foreground - ignored -->
  <style name="Occurrences" background="#707070"/>
  <style name="Occurrences.Rename" background="#202020"/>
  <!-- unused variable: foreground - color of underlining -->
  <style name="Occurrences.Unused" foreground="#006565"/>

  <!-- language primitives -->
  <style name="Number" foreground="#006565"/>
  <style name="String" foreground="#730073"/>
  <style name="Keyword" foreground="#000090"/>
  <style name="Operator" foreground="#000000"/>
  <style name="Preprocessor" foreground="#007300"/>
  <style name="Label" foreground="#840000"/>

  <!-- types, classes -->
  <style name="Type" foreground="#755B00"/>
  <style name="PrimitiveType" foreground="#000090"/>

  <!-- functions, methods -->
  <style name="Function" foreground="#007300"/>
  <style name="VirtualMethod" foreground="#007300" italic="true"/>

  <!-- variables -->
  <style name="Field" foreground="#f8f8f8"/>
  <style name="Local" foreground="#000000"/>
  <style name="Global" foreground="#000000"/>
  <style name="Static" foreground="#000000"/>
  <style name="JsScopeVar" foreground="#000000" italic="true"/>
  <style name="JsImportVar" foreground="#000000" italic="true"/>
  <style name="JsGlobalVar" foreground="#000000" italic="true"/>

  <!-- whitespace -->
  <style name="VisualWhitespace" foreground="#d0d0d0"/>

  <!-- comments -->
  <style name="Comment" foreground="#707070"/>
  <style name="DisabledCode" foreground="#707070"/>
  <style name="Doxygen.Comment" foreground="#707070"/>
  <style name="Doxygen.Tag" foreground="#707070"/>

  <!-- QML -->
  <style name="QmlTypeId" foreground="#000090"/>
  <style name="Binding" foreground="#840000"/>
  <style name="QmlExternalId" foreground="#007300" italic="true"/>
  <style name="QmlExternalObjectProperty" foreground="#007300" italic="true"/>
  <style name="QmlLocalId" italic="true"/>
  <style name="QmlRootObjectProperty" italic="true"/>
  <style name="QmlScopeObjectProperty" italic="true"/>
  <style name="QmlStateName" italic="true"/>

  <!-- inline diff -->
  <style name="DiffFile" foreground="#000090"/>
  <style name="DiffLocation" foreground="#007300"/>
  <style name="AddedLine" foreground="#730073"/>
  <style name="RemovedLine" foreground="#755B00"/>
  <style name="LogChangeLine" foreground="#840000"/>

  <!-- side-by-side diff -->
  <style name="DiffFileLine" foreground="#000090"/>
  <style name="DiffContextLine" foreground="#007300"/>
  <style name="DiffSourceLine" foreground="#730073"/>
  <style name="DiffSourceChar" foreground="#755B00" background="#d0dad0"/>
  <style name="DiffDestLine" foreground="#730073"/>
  <style name="DiffDestChar" foreground="#755B00" background="#d0dad0"/>

  <!-- errors, warnings -->
  <style name="Error" underlineColor="#840000" underlineStyle="SingleUnderline"/>
  <style name="ErrorContext" underlineColor="#840000" underlineStyle="DotLine"/>
  <style name="Warning" underlineColor="#755B00" underlineStyle="SingleUnderline"/>
  <style name="WarningContext" underlineColor="#755B00" underlineStyle="DotLine"/>
</style-scheme>
