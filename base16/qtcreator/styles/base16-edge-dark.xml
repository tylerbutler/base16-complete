<?xml version="1.0" encoding="UTF-8"?>
<style-scheme version="1.0" name="Base16 Edge Dark">
  <!--
  base16-qtcreator (https://github.com/ilpianista/base16-qtcreator)
  by Chris Kempson (http://chriskempson.com)
  Edge Dark scheme by cjayross (https://github.com/cjayross)
  -->

  <!-- main colors, skipping one of attributes (backgroud/foreground) will use color from here -->
  <style name="Text" foreground="#b7bec9" background="#262729"/>

  <!-- it will be underlined by default -->
  <style name="Link"/>

  <!-- selection, current line, search, matching brackets -->
  <style name="Selection" background="#b7bec9"/>
  <style name="CurrentLine" background="#3e4249"/>
  <style name="LineNumber" foreground="#3e4249"/>
  <style name="CurrentLineNumber" foreground="#73b3e7" bold="true"/>
  <style name="SearchResult" background="#88909f"/>
  <style name="Parentheses" background="#b7bec9" />
  <style name="ParenthesesMismatch" background="#d390e7" />

  <!-- Selection whilst searching in it -->
  <style name="SearchScope" foreground="#3e4249" background="#3e4249"/>

  <!-- word (symbol) under cursor: background - color of marking frame, foreground - ignored -->
  <style name="Occurrences" background="#3e4249"/>
  <style name="Occurrences.Rename" background="#73b3e7"/>
  <!-- unused variable: foreground - color of underlining -->
  <style name="Occurrences.Unused" foreground="#e77171"/>

  <!-- language primitives -->
  <style name="Number" foreground="#e77171"/>
  <style name="String" foreground="#a1bf78"/>
  <style name="Keyword" foreground="#d390e7"/>
  <style name="Operator" foreground="#b7bec9"/>
  <style name="Preprocessor" foreground="#73b3e7"/>
  <style name="Label" foreground="#e77171"/>

  <!-- types, classes -->
  <style name="Type" foreground="#dbb774"/>
  <style name="PrimitiveType" foreground="#d390e7"/>

  <!-- functions, methods -->
  <style name="Function" foreground="#73b3e7"/>
  <style name="VirtualMethod" foreground="#73b3e7" italic="true"/>

  <!-- variables -->
  <style name="Field" foreground="#d390e7"/>
  <style name="Local" foreground="#b7bec9"/>
  <style name="Global" foreground="#b7bec9"/>
  <style name="Static" foreground="#b7bec9"/>
  <style name="JsScopeVar" foreground="#b7bec9" italic="true"/>
  <style name="JsImportVar" foreground="#b7bec9" italic="true"/>
  <style name="JsGlobalVar" foreground="#b7bec9" italic="true"/>

  <!-- whitespace -->
  <style name="VisualWhitespace" foreground="#b7bec9"/>

  <!-- comments -->
  <style name="Comment" foreground="#3e4249"/>
  <style name="DisabledCode" foreground="#3e4249"/>
  <style name="Doxygen.Comment" foreground="#3e4249"/>
  <style name="Doxygen.Tag" foreground="#3e4249"/>

  <!-- QML -->
  <style name="QmlTypeId" foreground="#d390e7"/>
  <style name="Binding" foreground="#e77171"/>
  <style name="QmlExternalId" foreground="#73b3e7" italic="true"/>
  <style name="QmlExternalObjectProperty" foreground="#73b3e7" italic="true"/>
  <style name="QmlLocalId" italic="true"/>
  <style name="QmlRootObjectProperty" italic="true"/>
  <style name="QmlScopeObjectProperty" italic="true"/>
  <style name="QmlStateName" italic="true"/>

  <!-- inline diff -->
  <style name="DiffFile" foreground="#d390e7"/>
  <style name="DiffLocation" foreground="#73b3e7"/>
  <style name="AddedLine" foreground="#a1bf78"/>
  <style name="RemovedLine" foreground="#dbb774"/>
  <style name="LogChangeLine" foreground="#e77171"/>

  <!-- side-by-side diff -->
  <style name="DiffFileLine" foreground="#d390e7"/>
  <style name="DiffContextLine" foreground="#73b3e7"/>
  <style name="DiffSourceLine" foreground="#a1bf78"/>
  <style name="DiffSourceChar" foreground="#dbb774" background="#88909f"/>
  <style name="DiffDestLine" foreground="#a1bf78"/>
  <style name="DiffDestChar" foreground="#dbb774" background="#88909f"/>

  <!-- errors, warnings -->
  <style name="Error" underlineColor="#e77171" underlineStyle="SingleUnderline"/>
  <style name="ErrorContext" underlineColor="#e77171" underlineStyle="DotLine"/>
  <style name="Warning" underlineColor="#dbb774" underlineStyle="SingleUnderline"/>
  <style name="WarningContext" underlineColor="#dbb774" underlineStyle="DotLine"/>
</style-scheme>
