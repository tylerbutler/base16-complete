! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #282828, #282828, #1c1c1c

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #282828, #555555, #282828, #282828, #d1d1d1
rofi.color-active:  #282828, #2fb1d4, #282828, #282828, #2fb1d4
rofi.color-urgent:  #282828, #ff3d81, #282828, #282828, #ff3d81

! Set the desired separator style
rofi.separator-style: solid
