! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #282b36, #282b36, #1b1d24

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #282b36, #555e70, #282b36, #282b36, #f4f4f7
rofi.color-active:  #282b36, #2fb1d4, #282b36, #282b36, #2fb1d4
rofi.color-urgent:  #282b36, #ff3d81, #282b36, #282b36, #ff3d81

! Set the desired separator style
rofi.separator-style: solid
