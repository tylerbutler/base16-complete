! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #222222, #222222, #181818

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #222222, #818181, #222222, #222222, #e9e9e9
rofi.color-active:  #222222, #00eaff, #222222, #222222, #00eaff
rofi.color-urgent:  #222222, #ff29a8, #222222, #222222, #ff29a8

! Set the desired separator style
rofi.separator-style: solid
