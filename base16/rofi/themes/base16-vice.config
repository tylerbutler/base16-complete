! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #22262d, #22262d, #17191E

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #22262d, #8b9cbe, #22262d, #22262d, #f4f4f7
rofi.color-active:  #22262d, #00eaff, #22262d, #22262d, #00eaff
rofi.color-urgent:  #22262d, #ff29a8, #22262d, #22262d, #ff29a8

! Set the desired separator style
rofi.separator-style: solid
