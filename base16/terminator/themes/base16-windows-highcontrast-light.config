# Base16 Windows High Contrast Light
# Author: Fergus Collins (https://github.com/C-Fergus)

[global_config]
  title_transmit_fg_color = "#545454"
  title_transmit_bg_color = "#fcfcfc"
  title_inactive_fg_color = "#545454"
  title_inactive_bg_color = "#c0c0c0"

[profiles]
  [[default]]
    use_theme_colors = False
    background_color = "#fcfcfc"
    foreground_color = "#545454"
    cursor_color = "#545454"
    palette = "#fcfcfc:#800000:#008000:#808000:#000080:#800080:#008080:#545454:#c0c0c0:#fcfc54:#e8e8e8:#d4d4d4:#7e7e7e:#2a2a2a:#54fc54:#000000"
