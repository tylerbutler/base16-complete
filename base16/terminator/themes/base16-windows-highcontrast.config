# Base16 Windows High Contrast
# Author: Fergus Collins (https://github.com/C-Fergus)

[global_config]
  title_transmit_fg_color = "#c0c0c0"
  title_transmit_bg_color = "#000000"
  title_inactive_fg_color = "#c0c0c0"
  title_inactive_bg_color = "#545454"

[profiles]
  [[default]]
    use_theme_colors = False
    background_color = "#000000"
    foreground_color = "#c0c0c0"
    cursor_color = "#c0c0c0"
    palette = "#000000:#fc5454:#54fc54:#fcfc54:#5454fc:#fc54fc:#54fcfc:#c0c0c0:#545454:#808000:#1C1C1C:#383838:#a2a2a2:#dedede:#008000:#fcfcfc"
